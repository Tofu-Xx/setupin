{
  "name": "setupin",
  "type": "module",
  "version": "2.2.1",
  "packageManager": "pnpm@9.12.3",
  "description": "Vue SFC? HTML! <script setup> in html",
  "author": "tofu-xx <tofu-xx@foxmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/tofu-xx/setupin#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tofu-xx/setupin.git"
  },
  "bugs": {
    "url": "https://github.com/tofu-xx/setupin/issues"
  },
  "keywords": [
    "vue3",
    "setup-script",
    "html",
    "sfc",
    "vue-cdn"
  ],
  "exports": {
    ".": {
      "import": "./dist/main.js"
    }
  },
  "main": "dist/main.js",
  "files": [
    "dist",
    "public"
  ],
  "scripts": {
    "build": "vite build",
    "watch": "chokidar 'src/*' -c 'vite build'",
    "release": "bumpp --no-push && nr build && pnpm publish",
    "upvue": "bash -c \"{ echo export; curl -L https://unpkg.com/vue@3/dist/vue.global.prod.js; } > src/assets/vue.js\"",
    "up": "taze major -I",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@antfu/ni": "^0.23.0",
    "@babel/parser": "^7.26.1",
    "@babel/types": "^7.26.0",
    "@types/node": "^22.8.1",
    "bumpp": "^9.7.1",
    "eslint": "^9.13.0",
    "eslint-plugin-format": "^0.1.2",
    "taze": "^0.17.2",
    "vite": "^5.4.10"
  }
}
